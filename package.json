{
  "name": "gql-apollo-mongo-photo-svr",
  "version": "0.1.3",
  "description": "GraphQL Apollo MongoDB server for Vue photo app",
  "main": "src/main.ts",
  "scripts": {
    "build": "webpack --config webpack.$NODE_ENV.js",
    "start": "npm-run-all --serial build start:server",
    "start:server": "node ./dist/server",
    "start:server:env": "node --require dotenv/config dist/server",
    "test": "echo \"Error: no test specified\" && exit 1",
    "commit": "git-cz -S",
    "lint": "eslint .",
    "styleguide:build": "...",
    "prettier:check": "prettier --check 'src/**/*.{js,jsx,ts,tsx,mdx}'",
    "validate": "run-s test lint prettier:check",
    "prerelease": "git-authors-cli && git add .",
    "release": "standard-version",
    "postrelease": "run-s release:*",
    "release:tags": "git push --follow-tags origin development",
    "release:github": "conventional-github-releaser -p angular",
    "ci:validate": "rm -rf node_modules && npm ci && npm run validate",
    "prepublishOnly": "npm run ci:validate && npm run build"
  },
  "repository": "git@github.com/darylwalsh/graphql-vue-apollo-mongo-photo-app-svr.git",
  "keywords": [
    "GraphQL",
    "Apollo",
    "Mongo",
    "Vue"
  ],
  "author": "Daryl",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/darylwalsh/graphql-vue-apollo-mongo-photo-app-svr/issues"
  },
  "homepage": "https://github.com/darylwalsh/graphql-vue-apollo-mongo-photo-app-svr#readme",
  "dependencies": {
    "apollo-server": "^2.9.12",
    "bcrypt": "^3.0.7",
    "dotenv": "^8.2.0",
    "graphql": "^14.5.8",
    "jsonwebtoken": "^8.5.1",
    "md5": "^2.2.1",
    "mongoose": "^5.7.12",
    "typescript": "^3.7.2"
  },
  "devDependencies": {
    "@babel/types": "^7.6.1",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@commitlint/travis-cli": "8.1.0",
    "@types/jest": "24.0.18",
    "@types/mongoose": "^5.5.32",
    "@types/node": "^12.12.14",
    "@types/webpack-env": "^1.14.1",
    "@typescript-eslint/eslint-plugin": "2.1.0",
    "@typescript-eslint/parser": "2.1.0",
    "awesome-typescript-loader": "5.2.1",
    "cache-loader": "4.1.0",
    "chalk": "2.4.2",
    "clean-webpack-plugin": "^3.0.0",
    "commitizen": "^4.0.3",
    "conventional-changelog-cli": "2.0.23",
    "conventional-github-releaser": "^3.1.3",
    "cz-adapter-eslint": "0.3.0",
    "cz-conventional-changelog": "3.0.2",
    "eslint": "^6.7.1",
    "eslint-config-prettier": "6.1.0",
    "eslint-import-resolver-typescript": "1.1.1",
    "eslint-plugin-eslint-comments": "3.1.2",
    "eslint-plugin-eslint-plugin": "2.1.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jest": "22.16.0",
    "eslint-plugin-json": "1.4.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-typescript": "0.14.0",
    "git-authors-cli": "^1.0.18",
    "graphql-cli": "^4.0.0-alpha-cc59d20.478",
    "husky": "^3.1.0",
    "jest": "24.9.0",
    "lint-staged": "^9.5.0",
    "node-fetch": "2.6.0",
    "nodemon": "^2.0.1",
    "now": "16.1.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "rimraf": "3.0.0",
    "scaffdog": "0.1.2",
    "standard-version": "^7.0.1",
    "thread-loader": "2.1.3",
    "ts-jest": "24.0.2",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.5.3",
    "tslint": "5.19.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/"
    ],
    "collectCoverage": true,
    "coverageReporters": [
      "html"
    ],
    "cacheDirectory": ".jest/cache"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/**/*.mdx": [
      "prettier --write",
      "git add"
    ],
    "src/**/*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "contributors": [
    "Daryl <daryl@blpc.us>"
  ]
}
